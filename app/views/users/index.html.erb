<div class="container px-5 px-sm-0">
 <div class="row">
  <div class="col-md-3">
<%= render 'userinfo', user: @user %>
<%= render 'list', book: @book %>
  </div>
<div class="col-md-8 offset-md-1">
<h1>Users</h1>

<table>
 <tr>
   <th>image</th>
   <th>name</th>
 </tr>
 <% @users.each do |user| %>
  <tr>
     <td>
      <% if user.profile_image.attached? %>
       <%= image_tag url_for(user.profile_image) %>
      <% else %>
       <%= image_tag user.get_profile_image(10,10) %>
      <% end %>
     </td>
   <td><%= user.name %></td>
   <td><%= link_to 'Show', user_path(user) %></td>
  </tr>
  <% end %>
</table>
  </div>
 </div>
</div>